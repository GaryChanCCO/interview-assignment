{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "node_modules": true,
        "src/**/*.js": true,
        "src/**/*.js.map": true
    },
    "json.schemas": [
        {
            "fileMatch": ["**/func.json"],
            "url": "./func.schema.json"
        }
    ]
}
